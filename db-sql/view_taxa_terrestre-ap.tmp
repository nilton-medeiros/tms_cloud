select `t1`.`tx_id` AS `tx_id`,`t1`.`emp_id` AS `emp_id`,`t1`.`clie_id` AS `clie_id`,`t1`.`cc_id` AS `cc_id`,`t1`.`cid_id` AS `cid_id`,`t1`.`tx_nota` AS `tx_nota`,`t1`.`tx_por_peso` AS `tx_por_peso`,`t1`.`tx_valor` AS `tx_valor`,`t1`.`tx_ate_kg` AS `tx_ate_kg`,`t1`.`tx_excedente` AS `tx_excedente`,`t1`.`tx_cadastrado_por` AS `tx_cadastrado_por`,`t1`.`tx_cadastrado_em` AS `tx_cadastrado_em`,`t1`.`tx_alterado_por` AS `tx_alterado_por`,`t1`.`tx_alterado_em` AS `tx_alterado_em`,`t1`.`tx_versao` AS `tx_versao`,ifnull(`t4`.`clie_razao_social`,'(todos clientes)') AS `clie_razao_social`,`t6`.`cc_titulo` AS `cc_titulo`,`t5`.`cid_codigo_municipio` AS `cid_destino_codigo_municipio`,`t5`.`cid_municipio` AS `cid_destino_municipio`,`t5`.`cid_uf` AS `cid_destino_uf`,`t5`.`cid_sigla` AS `cid_destino_sigla`,`t5`.`cid_nome_aeroporto` AS `cid_destino_nome_aeroporto`,concat(`t5`.`cid_uf`,' - ',`t5`.`cid_municipio`,if(((`t5`.`cid_nome_aeroporto` is not null) and (`t5`.`cid_nome_aeroporto` <> '') and (`t5`.`cid_sigla` is not null) and (`t5`.`cid_sigla` <> '')),concat(' (',`t5`.`cid_sigla`,' - ',`t5`.`cid_nome_aeroporto`,')'),'')) AS `cid_destino_nome`,`t2`.`user_nome` AS `tx_cadastrado_por_nome`,`t3`.`user_nome` AS `tx_alterado_por_nome` from (((((`taxas` `t1` left join `usuarios` `t2` on((`t2`.`user_id` = `t1`.`tx_cadastrado_por`))) left join `usuarios` `t3` on((`t3`.`user_id` = `t1`.`tx_alterado_por`))) left join `clientes` `t4` on((`t4`.`clie_id` = `t1`.`clie_id`))) left join `cidades` `t5` on((`t5`.`cid_id` = `t1`.`cid_id`))) left join `composicao_calculo` `t6` on((`t6`.`cc_id` = `t1`.`cc_id`)))